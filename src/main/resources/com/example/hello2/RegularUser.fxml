        <Tab text="Main">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <Text fx:id="Welcome" fill="WHITE" layoutX="272.0" layoutY="46.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
               <font>
                  <Font size="30.0" />
               </font></Text>
                <Text fx:id="Balance" fill="WHITE" layoutX="289.0" layoutY="299.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />



                <Button layoutX="45.0" layoutY="275.0" mnemonicParsing="false" onAction="#rentItems" style="-fx-background-color: pink; -fx-text-fill: black; -fx-font-size: 18px; -fx-pref-width: 150px; -fx-pref-height: 40px;" text="Rent" />
