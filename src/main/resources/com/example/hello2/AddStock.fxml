<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hello2.Controller.Items.AddStock">
   <children>
      <ChoiceBox fx:id="Items" layoutX="224.0" layoutY="42.0" prefWidth="150.0" />
      <Label layoutX="216.0" layoutY="14.0" text="Choose an item to add stock to" />
      <TextArea fx:id="itemDetailsArea" layoutX="116.0" layoutY="107.0" prefHeight="166.0" prefWidth="365.0" />
      <TextField fx:id="Number" layoutX="210.0" layoutY="288.0" prefHeight="26.0" prefWidth="179.0" promptText="Enter Number Of stock To Add" />
      <Button fx:id="saveChanges" layoutX="264.0" layoutY="321.0" mnemonicParsing="false" onAction="#saveChanges" text="Save Changes" />
      <Button fx:id="back" layoutX="283.0" layoutY="354.0" mnemonicParsing="false" onAction="#Back" text="back" />
      <Button fx:id="ShowItemInfo" layoutX="250.0" layoutY="75.0" mnemonicParsing="false" onAction="#ShowItemInfo" text="Show Item Info" />
   </children>
</AnchorPane>
