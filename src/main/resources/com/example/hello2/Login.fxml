<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hello2.Controller.LoginPage">
   <TextField fx:id="IDField" layoutX="170.0" layoutY="119.0" prefHeight="40.0" prefWidth="280.0" promptText="Enter Your ID" />
   <PasswordField fx:id="passwordField" layoutX="170.0" layoutY="180.0" prefHeight="40.0" prefWidth="280.0" promptText="Enter Your Password" />
   <Button fx:id="LogIn" layoutX="240.0" layoutY="249.0" minWidth="120.0" onAction="#handleLogIn" styleClass="primary-button" text="Log In" />
   <Button fx:id="back" layoutX="278.0" layoutY="360.0" mnemonicParsing="false" onAction="#Back" text="Back" />
</AnchorPane>
