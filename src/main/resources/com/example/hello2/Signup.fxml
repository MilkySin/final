<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hello2.Controller.SignupPage">

   <TextField fx:id="usernameField" layoutX="375.0" layoutY="60.0" prefHeight="40.0" prefWidth="210.0" promptText="Enter Your Name" />
   <PasswordField fx:id="passwordField" layoutX="14.0" layoutY="135.0" prefHeight="40.0" prefWidth="210.0" promptText="Enter your password" />
   <Button layoutX="457.0" layoutY="346.0" mnemonicParsing="false" onAction="#signup" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: #4CAF50; -fx-text-fill: white; -fx-font-size: 16px;" text="Done" />
   <TextField fx:id="IDField" layoutX="14.0" layoutY="60.0" prefHeight="40.0" prefWidth="210.0" promptText="Enter Your ID" />
   <TextField fx:id="AddressField" layoutX="375.0" layoutY="135.0" prefHeight="40.0" prefWidth="210.0" promptText="Enter Your Address" />
   <TextField fx:id="NumberField" layoutX="14.0" layoutY="211.0" prefHeight="40.0" prefWidth="210.0" promptText="Enter Your Phone Number" />
   <Button fx:id="back" layoutX="75.0" layoutY="346.0" mnemonicParsing="false" onAction="#Back" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: #F44336; -fx-text-fill: white; -fx-font-size: 16px;" text="back" />
   <ChoiceBox fx:id="accountTypeChoiceBox" layoutX="375.0" layoutY="211.0" prefHeight="40.0" prefWidth="210.0">

   </ChoiceBox>


</AnchorPane>
