<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #515151; -fx-border-color: white;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hello2.Controller.Display.ItemSelectVIPController">
    <TabPane layoutX="2.0" prefHeight="400.0" prefWidth="600.0" style="-fx-padding: 0 0 0 0;" stylesheets="@CSS/styles.css" tabClosingPolicy="UNAVAILABLE">
        <Tab text="Main">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <Text fx:id="Welcome" fill="WHITE" layoutX="23.0" layoutY="41.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                <Text fx:id="Balance" fill="WHITE" layoutX="23.0" layoutY="58.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />

                <Button layoutX="34.0" layoutY="79.0" mnemonicParsing="false" onAction="#rentItems" style="-fx-background-color: pink; -fx-text-fill: black; -fx-font-size: 18px; -fx-pref-width: 150px; -fx-pref-height: 40px;" text="Rent" />
                <Button layoutX="34.0" layoutY="150.0" mnemonicParsing="false" onAction="#Return" style="-fx-background-color: pink; -fx-text-fill: black; -fx-font-size: 18px; -fx-pref-width: 150px; -fx-pref-height: 40px;" text="Return" />

                <Label fx:id="Account" alignment="CENTER" contentDisplay="CENTER" layoutX="300.0" layoutY="31.0" style="-fx-font-size: 24px; -fx-font-weight: bold; -fx-text-fill: white;" text="This is a VIP Account" textAlignment="CENTER" />
                <Button fx:id="back" layoutX="81.0" layoutY="291.0" mnemonicParsing="false" onAction="#Back" style="-fx-background-color: pink;" text="Logout" />
                <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="343.0" layoutY="115.0" pickOnBounds="true" preserveRatio="true">
                    <Image url="@Images/Windows_10_Default_Profile_Picture.svg.png" />
                </ImageView>
                <Button fx:id="freebutton" layoutX="34.0" layoutY="225.0" mnemonicParsing="false" onAction="#freeItem" style="-fx-background-color: pink; -fx-text-fill: black; -fx-font-size: 18px; -fx-pref-width: 150px; -fx-pref-height: 40px;" text="Free Items" />
                <Text fx:id="Points" fill="WHITE" layoutX="23.0" layoutY="19.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
            </AnchorPane>
        </Tab>
        <Tab onSelectionChanged="#ownedItems" text="Owned">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <ScrollPane fx:id="ownedItemsDisplay" hbarPolicy="NEVER" prefHeight="370.0" prefWidth="600.0" />
            </AnchorPane>
        </Tab>
    </TabPane>

</AnchorPane>
